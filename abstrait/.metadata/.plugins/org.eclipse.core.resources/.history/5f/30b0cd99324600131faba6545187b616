
public class Enfant extends Personne{
	private int age;

	public int getAge() {
		return this.age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	Enfant(String nom, String prenom, int age)
	{
		super(nom, prenom);
		this.setAge(age);
	}
	
	public void afficher()
	{
		
	}
	
	public void saisir()
	{
		
	}
	
}
