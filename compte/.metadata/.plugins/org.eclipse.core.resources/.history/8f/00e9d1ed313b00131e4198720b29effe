
public class Banque {
	
	private Compte comptes[];
	
	public Banque() {
		this.setComptes(new Compte());
	}
	
	/**
	 * Setter
	 * @param Compte compte Objet Compte ˆ rentrer dans le tableau des comptes
	 */
	public void setComptes(Compte compte) {
		this.comptes[] = new Compte();
	}
	
	/**
	 * Getter
	 * @return Compte comptes Tableau contenant des objets Compte
	 */
	public Compte[] getComptes() {
		return this.comptes;
	}
	
	public static void main(String[] args) {
		
	}
}
